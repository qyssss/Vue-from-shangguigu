{"ast":null,"code":"import { ref, reactive, toRefs, toRaw } from \"vue\";\nexport default {\n  name: \"Demo\",\n\n  setup() {\n    // 数据\n    let person = reactive({\n      name: \"qys\",\n      age: 14,\n      job: {\n        j1: {\n          salary: 2000\n        }\n      },\n      car: {}\n    });\n    let sum = ref(0);\n\n    function showRawPerson() {\n      // toRaw只能变reactive定义的响应式对象,变回原本的样子\n      const p = toRaw(person);\n      console.log(p);\n    } // 给响应式对象添加的数据都是响应式的\n\n\n    function addCar() {\n      let car = {\n        name: \"baoma\",\n        price: \"9000w\"\n      };\n      person.car = car;\n    }\n\n    return {\n      sum,\n      person,\n      ...toRefs(person),\n      showRawPerson,\n      addCar\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,SAASA,GAAT,EAAcC,QAAd,EAAwBC,MAAxB,EAAgCC,KAAhC,QAA6C,KAA7C;AACA,eAAe;EACbC,IAAI,EAAE,MADO;;EAEbC,KAAK,GAAG;IACN;IACA,IAAIC,MAAK,GAAIL,QAAQ,CAAC;MACpBG,IAAI,EAAE,KADc;MAEpBG,GAAG,EAAE,EAFe;MAGpBC,GAAG,EAAE;QACHC,EAAE,EAAE;UACFC,MAAM,EAAE;QADN;MADD,CAHe;MAQpBC,GAAG,EAAE;IARe,CAAD,CAArB;IAUA,IAAIC,GAAE,GAAIZ,GAAG,CAAC,CAAD,CAAb;;IACA,SAASa,aAAT,GAAyB;MACvB;MACA,MAAMC,IAAIX,KAAK,CAACG,MAAD,CAAf;MACAS,OAAO,CAACC,GAAR,CAAYF,CAAZ;IACF,CAjBM,CAkBN;;;IACA,SAASG,MAAT,GAAkB;MAChB,IAAIN,GAAE,GAAI;QAAEP,IAAI,EAAE,OAAR;QAAiBc,KAAK,EAAE;MAAxB,CAAV;MACAZ,MAAM,CAACK,GAAP,GAAaA,GAAb;IACF;;IACA,OAAO;MACLC,GADK;MAELN,MAFK;MAGL,GAAGJ,MAAM,CAACI,MAAD,CAHJ;MAILO,aAJK;MAKLI;IALK,CAAP;EAOD;;AAhCY,CAAf","names":["ref","reactive","toRefs","toRaw","name","setup","person","age","job","j1","salary","car","sum","showRawPerson","p","console","log","addCar","price"],"sourceRoot":"","sources":["D:\\尚硅谷-vue\\vue3__test\\src\\components\\Demo.vue"],"sourcesContent":["<template>\r\n  <h2>sum:{{ sum }}</h2>\r\n  <button @click=\"sum++\">++</button>\r\n  <h2>姓名：{{ name }}</h2>\r\n  <h2>年龄：{{ age }}</h2>\r\n  <h2>薪水:{{ job.j1.salary }}</h2>\r\n  <h2>car:{{ person.car }}</h2>\r\n  <button @click=\"name += '!'\">修改姓名</button>\r\n  <button @click=\"age++\">修改年龄</button>\r\n  <button @click=\"job.j1.salary++\">薪资++</button>\r\n  <button @click=\"showRawPerson\">输出原始person</button>\r\n  <button @click=\"addCar\">加车</button>\r\n  <button @click=\"person.car.name+='!'\">换车</button>\r\n  <button @click=\"person.car.price++\">++</button>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, toRefs, toRaw } from \"vue\";\r\nexport default {\r\n  name: \"Demo\",\r\n  setup() {\r\n    // 数据\r\n    let person = reactive({\r\n      name: \"qys\",\r\n      age: 14,\r\n      job: {\r\n        j1: {\r\n          salary: 2000,\r\n        },\r\n      },\r\n      car: {},\r\n    });\r\n    let sum = ref(0);\r\n    function showRawPerson() {\r\n      // toRaw只能变reactive定义的响应式对象,变回原本的样子\r\n      const p = toRaw(person);\r\n      console.log(p);\r\n    }\r\n    // 给响应式对象添加的数据都是响应式的\r\n    function addCar() {\r\n      let car = { name: \"baoma\", price: \"9000w\" };\r\n      person.car = car;\r\n    }\r\n    return {\r\n      sum,\r\n      person,\r\n      ...toRefs(person),\r\n      showRawPerson,\r\n      addCar,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n}\r\n</style>\r\n\r\n"]},"metadata":{},"sourceType":"module"}