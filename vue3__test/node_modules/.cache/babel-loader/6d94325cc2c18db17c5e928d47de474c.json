{"ast":null,"code":"import { ref, reactive } from \"vue\";\nexport default {\n  name: \"App\",\n\n  setup() {\n    // 数据  ref 把普通数据变成响应式的引用对象(RefImpl)\n    // ref 处理基本数据用的getter,setter(Object.defineProperty),\n    // 处理对象转化为Proxy(封装在reactive)\n    let name = ref(\"sqy\");\n    let age = ref(18); // reactive 只能用于对象类型,直接把对象变成Proxy类型\n\n    let job = reactive({\n      type: \"student\",\n      salary: \"10k\"\n    });\n    let hobby = ['dance', 'sing']; // 方法\n\n    function changeInfo() {\n      // ref 生成了一个引用对象,用value访问值,在模板中不要用.value,vue会自动做\n      name.value = \"qys\";\n      age.value = 88; // 对象中内部也配置了响应式,但是不需要.value\n\n      job.type = \"clerk\";\n      job.salary = \"28k\";\n      console.log(job);\n    }\n\n    return {\n      name,\n      age,\n      changeInfo,\n      job\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAUA,SAASA,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,eAAe;EACbC,IAAI,EAAE,KADO;;EAEbC,KAAK,GAAG;IACN;IACA;IACA;IACA,IAAID,IAAG,GAAIF,GAAG,CAAC,KAAD,CAAd;IACA,IAAII,GAAE,GAAIJ,GAAG,CAAC,EAAD,CAAb,CALM,CAMN;;IACA,IAAIK,GAAE,GAAIJ,QAAQ,CAAC;MACjBK,IAAI,EAAE,SADW;MAEjBC,MAAM,EAAE;IAFS,CAAD,CAAlB;IAIA,IAAIC,KAAI,GAAI,CAAC,OAAD,EAAS,MAAT,CAAZ,CAXM,CAYN;;IACA,SAASC,UAAT,GAAsB;MACpB;MACAP,IAAI,CAACQ,KAAL,GAAa,KAAb;MACAN,GAAG,CAACM,KAAJ,GAAY,EAAZ,CAHoB,CAIpB;;MACAL,GAAG,CAACC,IAAJ,GAAW,OAAX;MACAD,GAAG,CAACE,MAAJ,GAAa,KAAb;MACAI,OAAO,CAACC,GAAR,CAAYP,GAAZ;IACF;;IACA,OAAO;MACLH,IADK;MAELE,GAFK;MAGLK,UAHK;MAILJ;IAJK,CAAP;EAMD;;AA9BY,CAAf","names":["ref","reactive","name","setup","age","job","type","salary","hobby","changeInfo","value","console","log"],"sourceRoot":"","sources":["D:\\尚硅谷-vue\\vue3_test\\src\\App.vue"],"sourcesContent":["<template>\n  <h1>信息</h1>\n  <h2>姓名{{ name }}</h2>\n  <h2>年龄{{ age }}</h2>\n  <h3>工作种类:{{ job.type }}</h3>\n  <h3>工作薪水:{{ job.salary }}</h3>\n  <button @click=\"changeInfo\">修改信息</button>\n</template>\n\n<script>\nimport { ref, reactive } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据  ref 把普通数据变成响应式的引用对象(RefImpl)\n    // ref 处理基本数据用的getter,setter(Object.defineProperty),\n    // 处理对象转化为Proxy(封装在reactive)\n    let name = ref(\"sqy\");\n    let age = ref(18);\n    // reactive 只能用于对象类型,直接把对象变成Proxy类型\n    let job = reactive({\n      type: \"student\",\n      salary: \"10k\",\n    });\n    let hobby = ['dance','sing']\n    // 方法\n    function changeInfo() {\n      // ref 生成了一个引用对象,用value访问值,在模板中不要用.value,vue会自动做\n      name.value = \"qys\";\n      age.value = 88;\n      // 对象中内部也配置了响应式,但是不需要.value\n      job.type = \"clerk\";\n      job.salary = \"28k\";\n      console.log(job)\n    }\n    return {\n      name,\n      age,\n      changeInfo,\n      job,\n    };\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}